      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="SELECT MA001,MA003,全年預算,當月預算,當月借方,當月貸方,累積預算,累積借方,累積貸方,(當月借方-當月貸方) AS '當月實際',(累積借方-累積貸方) AS '累積實際', (當月借方-當月貸方)/ISNULL(NULLIF(當月預算,0),1) AS '當月達成率',(累積借方-累積貸方)/ISNULL(NULLIF(累積預算,0),1) AS '累積達成率',(累積借方-累積貸方)/ISNULL(NULLIF(全年預算,0),1) AS '全年達成率'&#13;&#10;FROM (  &#13;&#10;SELECT MA001,MA003&#13;&#10;,(SELECT ISNULL(SUM(MK006),0) FROM [TK].dbo.ACTMK WHERE MK003 LIKE LTRIM(RTRIM(MA001))+'%' AND MK004&lt;&gt;'******' AND MK002=@P1 AND MK005=@P2) AS '當月預算' &#13;&#10;,(SELECT ISNULL(SUM(TB007),0) FROM [TK].dbo.ACTTA,[TK].dbo.ACTTB WHERE TA001=TB001 AND TA002=TB002 AND SUBSTRING(TA003,1,6)&gt;=@P3 AND SUBSTRING(TA003,1,6)&lt;=@P4 AND TB005 LIKE LTRIM(RTRIM(MA001))+'%' AND TB004='1' AND TB016='Y'  ) AS '當月借方'&#13;&#10;,(SELECT ISNULL(SUM(TB007),0) FROM [TK].dbo.ACTTA,[TK].dbo.ACTTB WHERE TA001=TB001 AND TA002=TB002 AND SUBSTRING(TA003,1,6)&gt;=@P3 AND SUBSTRING(TA003,1,6)&lt;=@P4 AND TB005 LIKE LTRIM(RTRIM(MA001))+'%' AND TB004='-1' AND TB016='Y'  )  AS '當月貸方'&#13;&#10;,(SELECT ISNULL(SUM(MK006),0) FROM [TK].dbo.ACTMK WHERE MK003 LIKE LTRIM(RTRIM(MA001))+'%' AND MK004&lt;&gt;'******' AND MK002=@P1 AND MK005&lt;=@P2) AS '累積預算' &#13;&#10;,(SELECT ISNULL(SUM(TB007),0) FROM [TK].dbo.ACTTA,[TK].dbo.ACTTB WHERE TA001=TB001 AND TA002=TB002 AND SUBSTRING(TA003,1,6)&gt;=@P5 AND SUBSTRING(TA003,1,6)&lt;=@P6 AND TB005 LIKE LTRIM(RTRIM(MA001))+'%' AND TB004='1' AND TB016='Y'  ) AS '累積借方'&#13;&#10;,(SELECT ISNULL(SUM(TB007),0) FROM [TK].dbo.ACTTA,[TK].dbo.ACTTB WHERE TA001=TB001 AND TA002=TB002 AND SUBSTRING(TA003,1,6)&gt;=@P5 AND SUBSTRING(TA003,1,6)&lt;=@P6 AND TB005 LIKE LTRIM(RTRIM(MA001))+'%' AND TB004='-1'   AND TB016='Y')  AS '累積貸方'&#13;&#10;&#13;&#10;,(SELECT ISNULL(SUM(MK006),0) FROM [TK].dbo.ACTMK WHERE MK003 LIKE LTRIM(RTRIM(MA001))+'%' AND MK004&lt;&gt;'******' AND MK002='2017') AS '全年預算' &#13;&#10;FROM [TK].dbo.ACTMA    &#13;&#10;WHERE ((MA001 LIKE '6%') OR  (MA001 LIKE '51112%' ) OR  (MA001 LIKE '51113%' ))&#13;&#10;AND MA001&gt;=@P11 AND MA001&lt;=@P12&#13;&#10;AND MA008&lt;&gt;'1')&#13;&#10;AS TEMP&#13;&#10;ORDER BY MA001">
