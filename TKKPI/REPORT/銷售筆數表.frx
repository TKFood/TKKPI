      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="SELECT &#13;&#10;*&#13;&#10;,(CASE WHEN 單筆消費滿額的筆數&gt;0 AND 總消費筆數&gt;0 THEN CONVERT(DECIMAL,單筆消費滿額的筆數)/ CONVERT(DECIMAL,總消費筆數) ELSE 0 END ) AS 'PCTS'&#13;&#10;FROM &#13;&#10;(&#13;&#10;	SELECT &#13;&#10;	'20251001'+'~'+'20251102'  AS '日期',&#13;&#10;	TA002 AS '門市代',&#13;&#10;	ME002 AS '門市',&#13;&#10;	COUNT(TA002) AS '總消費筆數',&#13;&#10;	SUM(TA024) AS '總銷售額(含稅)',&#13;&#10;	(&#13;&#10;	SELECT COUNT(TA002)&#13;&#10;	FROM [TK].dbo.POSTA TA2&#13;&#10;	WHERE 1=1&#13;&#10;	AND TA2.TA002=POSTA.TA002&#13;&#10;	AND TA001&gt;='20251001' AND TA001&lt;='20251102'&#13;&#10;	AND TA024&gt;=699&#13;&#10;	) AS '單筆消費滿額的筆數'&#13;&#10;	FROM [TK].dbo.POSTA,[TK].dbo.CMSME&#13;&#10;	WHERE 1=1&#13;&#10;	AND TA002=ME001                                &#13;&#10;	AND TA001&gt;='20251001' AND TA001&lt;='20251102'&#13;&#10;	GROUP BY TA002,ME002&#13;&#10;) AS TEMP&#13;&#10;ORDER BY 門市代,門市">
