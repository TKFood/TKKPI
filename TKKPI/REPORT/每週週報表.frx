        <Column Name="年月" DataType="System.String"/>
        <Column Name="部門" DataType="System.String"/>
        <Column Name="業務員" DataType="System.String"/>
        <Column Name="未出金額" DataType="System.Decimal"/>
        <Column Name="本幣金額" DataType="System.Int32"/>
      </TableDataSource>
